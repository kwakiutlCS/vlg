<%= form_for [:admin, @material] do |f| %>

<p>
  <% @material.errors.full_messages.each do |msg| %>
  <%= msg %><br />
  <% end %>
</p> 


<p>
  <%= f.label :name %>
  <%= f.text_field :name %>
</p>

<p>
  <%= f.label :mass %>
  <%= f.number_field :mass %>
</p>

<p>
  <%= f.label :decay %>
  <%= f.text_field :decay %>
</p>

<p>
  <%= f.label :decay_probability %>
  <%= f.number_field :decay_probability %>
</p>


<p>
  <%= f.label :function %>
  <%= f.text_field :function %>
</p>



<p>
  <%= f.label :quality %>
  <%= f.number_field :quality %>
</p>






<%= f.submit %>

<% end %>
